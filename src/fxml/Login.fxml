<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="230.0" prefWidth="425.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.LoginController">
   <children>
      <TextField fx:id="usernameTextField" layoutX="31.0" layoutY="86.0" />
      <TextField fx:id="passwordTextField" layoutX="246.0" layoutY="86.0" />
      <Button fx:id="loginButton" alignment="CENTER" contentDisplay="CENTER" layoutX="157.0" layoutY="140.0" mnemonicParsing="false" onAction="#submitLogin" text="%loginButtonText" />
      <Label fx:id="usernameLabel" layoutX="31.0" layoutY="69.0" text="%usernameText" />
      <Label fx:id="passwordLabel" layoutX="246.0" layoutY="69.0" text="%passwordText" />
      <Label fx:id="appNameLabel" layoutX="32.0" layoutY="14.0" text="%appNameText">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <Label fx:id="errorLabel" layoutX="83.0" layoutY="115.0" prefHeight="17.0" prefWidth="260.0" textFill="RED" />
      <Label fx:id="locationLabel" layoutX="231.0" layoutY="199.0" prefHeight="17.0" prefWidth="164.0" />
      <Label fx:id="accessLocationLabel" alignment="CENTER_RIGHT" layoutX="32.0" layoutY="199.0" prefHeight="17.0" prefWidth="117.0" text="%accessLocationText" />
   </children>
</AnchorPane>
